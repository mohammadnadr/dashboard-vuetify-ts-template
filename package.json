{
  "name": "NADR-vuetify-vuejs",
  "version": "1.2.0",
  "private": true,
  "author": "MohammadHossein",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-stage": "vue-tsc --noEmit && vite build --base=/vue/stage/",
    "build-prod": "vue-tsc --noEmit && vite build --base=/vue/",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-pattern .gitignore"
  },
  "dependencies": {
    "@ant-design/icons-vue": "7.0.1",
    "@eslint/js": "9.14.0",
    "@fontsource/inter": "5.1.0",
    "@fontsource/poppins": "5.1.0",
    "@fontsource/public-sans": "5.1.1",
    "@fontsource/roboto": "5.1.0",
    "@mdi/js": "^7.4.47",
    "@tsconfig/node20": "^20.1.4",
    "@typescript-eslint/parser": "8.14.0",
    "ant-design-vue": "4.2.6",
    "axios": "^1.7.9",
    "pinia": "2.2.6",
    "typescript-eslint": "8.14.0",
    "vee-validate": "4.14.7",
    "vite-plugin-vuetify": "2.0.4",
    "vue": "3.5.12",
    "vue-i18n": "10.0.4",
    "vue-recaptcha-v3": "^2.0.1",
    "vue-router": "4.4.5",
    "vue-tabler-icons": "2.21.0",
    "vue3-perfect-scrollbar": "2.0.0",
    "vuetify": "3.7.4",
    "webpack-plugin-vuetify": "3.0.3"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
    "@types/node": "22.9.0",
    "@vitejs/plugin-vue": "5.2.0",
    "@vue/tsconfig": "0.6.0",
    "eslint": "9.14.0",
    "eslint-plugin-vue": "9.31.0",
    "prettier": "3.3.3",
    "sass": "1.77.8",
    "sass-loader": "16.0.0",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vue-cli-plugin-vuetify": "2.5.8",
    "vue-tsc": "2.1.10"
  }
}
